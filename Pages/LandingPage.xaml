<Page x:Class="GameOn.LandingPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GameOn"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="LandingPage">

    <Grid>

        <Frame x:Name="LandingFrame"></Frame>
        <ScrollViewer VerticalScrollBarVisibility="Hidden">
            <Image Source="/img/Landing Page.png" ></Image>
        </ScrollViewer>

        <Button Click="Button_Profil" Margin="737,20,20,377">
            <Image Source="/img/Profil.png" Width="40" Height="40" />
        </Button>
    </Grid>
</Page>
